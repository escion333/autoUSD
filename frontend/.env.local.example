# Copy this file to .env.local and fill in the values

# RPC URLs (Required)
NEXT_PUBLIC_BASE_RPC_URL=https://sepolia.base.org
NEXT_PUBLIC_KATANA_RPC_URL=http://localhost:8545
NEXT_PUBLIC_ZIRCUIT_RPC_URL=https://zircuit-testnet.drpc.org

# API Keys (Optional - will use mocks if not provided)
NEXT_PUBLIC_CIRCLE_API_KEY=
NEXT_PUBLIC_HYPERLANE_API_KEY=

# Encryption Key (Optional - uses default in dev, required in production)
NEXT_PUBLIC_ENCRYPTION_KEY=your-secret-encryption-key-here

# Development settings
NODE_ENV=development